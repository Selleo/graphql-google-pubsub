    "integration": "mocha --reporter spec --full-trace ./dist/test/integration-tests.js"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.1.0",
    "graphql-subscriptions": "^2.0.0",
    "iterall": "^1.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/simple-mock": "^0.8.6",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "mocha": "^10.2.0",
    "simple-mock": "^0.8.0",
    "tslint": "^5.20.1",
    "typescript": "^5.3.3"
  }
}
